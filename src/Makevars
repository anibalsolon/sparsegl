F_OBJS = spmatmul.o sparsegl.o sgl_subfuns.o log_sgl_subfuns.o sparsegllog.o


all: $(SHLIB) clean

$(SHLIB): $(F_OBJS)

spmatmul.mod: spmatmul.o
sgl_subfuns.mod: spmatmul.mod
log_sgl_subfuns.mod: spmatmul.mod
sparsegl.o: spmatmul.mod sgl_subfuns.mod
sparsegllog.o: log_sgl_subfuns.mod sgl_subfuns.mod spmatmul.mod

clean:
	@rm -rf *.mod *.o
